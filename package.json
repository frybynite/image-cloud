{
  "name": "@frybynite/image-cloud",
  "version": "0.5.1",
  "description": "Javascript/TypeScript image cloud library with custom loaders, various layouts, full images styling, with animation and zoom effects",
  "type": "module",
  "main": "./dist/image-cloud.umd.js",
  "module": "./dist/image-cloud.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/image-cloud.js",
      "require": "./dist/image-cloud.umd.js"
    },
    "./style.css": "./dist/style.css",
    "./auto-init": "./dist/image-cloud-auto-init.js",
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.js"
    },
    "./web-component": {
      "types": "./dist/web-component.d.ts",
      "import": "./dist/web-component.js"
    },
    "./layouts/radial": {
      "types": "./dist/layouts/radial.d.ts",
      "import": "./dist/layouts/radial.js"
    },
    "./layouts/grid": {
      "types": "./dist/layouts/grid.d.ts",
      "import": "./dist/layouts/grid.js"
    },
    "./layouts/spiral": {
      "types": "./dist/layouts/spiral.d.ts",
      "import": "./dist/layouts/spiral.js"
    },
    "./layouts/cluster": {
      "types": "./dist/layouts/cluster.d.ts",
      "import": "./dist/layouts/cluster.js"
    },
    "./layouts/wave": {
      "types": "./dist/layouts/wave.d.ts",
      "import": "./dist/layouts/wave.js"
    },
    "./layouts/random": {
      "types": "./dist/layouts/random.d.ts",
      "import": "./dist/layouts/random.js"
    },
    "./layouts/all": {
      "types": "./dist/layouts/all.d.ts",
      "import": "./dist/layouts/all.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && tsc && vite build && vite build --config vite.image-cloud-auto-init.config.ts && vite build --config vite.layout-radial.config.ts && vite build --config vite.layout-grid.config.ts && vite build --config vite.layout-spiral.config.ts && vite build --config vite.layout-cluster.config.ts && vite build --config vite.layout-wave.config.ts && vite build --config vite.layout-random.config.ts && vite build --config vite.layout-all.config.ts && vite build --config vite.react.config.ts && vite build --config vite.vue.config.ts && vite build --config vite.web-component.config.ts",
    "test": "playwright test --config=test/playwright.config.ts",
    "test:ui": "playwright test --config=test/playwright.config.ts --ui",
    "test:headed": "playwright test --config=test/playwright.config.ts --headed",
    "test:update-snapshots": "playwright test --config=test/playwright.config.ts --update-snapshots",
    "build:dev": "./scripts/build.sh",
    "build:prod": "./scripts/build.sh --production",
    "build:analyze": "ANALYZE=true npm run build",
    "build:watch": "vite build --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist build",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "serve": "python3 -m http.server 8080",
    "pack": "npm run build && mkdir -p build/packages && npm pack && mv *.tgz build/packages/",
    "test:package": "./scripts/test-package.sh",
    "test:serve": "cd test-package && npx serve .",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "image-gallery",
    "typescript",
    "animation",
    "zoom",
    "interactive",
    "radial-layout",
    "google-drive"
  ],
  "author": "frybynite",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frybynite/image-cloud.git"
  },
  "bugs": {
    "url": "https://github.com/frybynite/image-cloud/issues"
  },
  "homepage": "https://github.com/frybynite/image-cloud#readme",
  "peerDependencies": {
    "@frybynite/image-cloud": "*",
    "react": ">=17",
    "react-dom": ">=17",
    "vue": ">=3"
  },
  "peerDependenciesMeta": {
    "react": { "optional": true },
    "react-dom": { "optional": true },
    "vue": { "optional": true }
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rollup-plugin-visualizer": "^5.x",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.28"
  }
}
